package java01;

import java.util.Arrays;
import java.util.List;
import java.util.Set;

public class Sorting {

	public static void main(String[] args) {
		
		String str="deepak";
		
		for(int i=0;i<str.length();i++) {
			
			char ch=str.charAt(i);
			
			
			System.out.println(ch);
		}
		
		
		
	}

}
